x;\"\n      @change=\"onPerPageChanged\"\n    >\n      <template #title>\n        <span class=\"text-xs text-gray-600 dark:text-gray-300\">\n          Item per Halaman\n        </span>\n      </template>\n    </InputSelect>\n    <span class=\"w-4\" />\n    <button\n      class=\"table-filter__btn-reset\"\n      :disabled=\"!canReset\"\n      @click=\"onReset\"\n    >\n      Reset\n    </button>\n  </div>\n</template>\n\n<script>\nimport _isEqual from 'lodash/isEqual'\nimport DateRangePicker from './date-range-picker.vue'\nimport InputSelect from '../Form/Select'\n\nconst defaultFilter = {\n  startDate: null,\n  endDate: null,\n  perPage: 10\n}\n\nexport default {\n  components: {\n    DateRangePicker,\n    InputSelect\n  },\n  props: {\n    filter: {\n      type: Object,\n      default: () => defaultFilter\n    }\n  },\n  data () {\n    return {\n      perPageOptions: [5, 10, 20]\n    }\n  },\n  computed: {\n    dateRange () {\n      const { startDate, endDate } = this.filter\n      const start = startDate ? new Date(startDate) : null\n      const end = endDate ? new Date(endDate) : null\n      return {\n        start,\n        end\n      }\n    },\n    canReset () {\n      return !_isEqual(defaultFilter, this.filter)\n    }\n  },\n  methods: {\n    onDateChanged ({ start_date: startDate, end_date: endDate }) {\n      this.$emit('change', {\n        ...this.filter,\n        startDate,\n        endDate\n      })\n    },\n    onPerPageChanged (perPage) {\n      this.$emit('change', {\n        ...this.filter,\n        perPage\n      })\n    },\n    onReset () {\n      this.$emit('change', { ...defaultFilter })\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.table-filter {\n  @apply flex justify-start items-start;\n\n  &__btn-reset {\n    @apply mt-6 px-4 py-3\n    flex justify-center items-center\n    rounded\n    bg-white\n    border border-blue-500\n    text-blue-500\n    leading-5;\n\n    &:focus {\n      @apply outline-none;\n    }\n\n    &:active {\n      @apply bg-blue-400;\n    }\n\n    &:disabled {\n      @apply cursor-not-allowed\n      text-gray-500\n      bg-gray-300 border-gray-500\n      opacity-50;\n    }\n  }\n}\n</style>\n"]}]}