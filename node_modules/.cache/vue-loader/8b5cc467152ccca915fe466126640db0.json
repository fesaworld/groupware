tle=\"subtitle\"\n        :required=\"required\"\n      >\n        <template #title>\n          <slot name=\"title\"></slot>\n        </template>\n        <template #subtitle>\n          <slot name=\"subtitle\"></slot>\n        </template>\n      </FormInputHeader>\n      <input class=\"hidden\" type=\"date\" :value=\"value\" :disabled=\"disabled\">\n    </ValidationProvider>\n    <DateTime\n      v-bind=\"$attrs\"\n      v-model=\"syncValue\"\n      :disabled=\"disabled\"\n      input-class=\"form-input__input\"/>\n    <p v-if=\"errors.length\"\n      class=\"form-input__error-hint\">\n      <slot name=\"error\">\n        {{errors[0]}}\n      </slot>\n    </p>\n  </div>\n</template>\n\n<script>\nimport { props, components } from './input-mixin'\nimport { Datetime as DateTime } from 'vue-datetime'\n\nexport default {\n  inheritAttrs: false,\n  components: {\n    ...components,\n    DateTime\n  },\n  model: {\n    prop: 'value',\n    event: 'change'\n  },\n  props: {\n    ...props,\n    value: {\n      type: [String, Number, Date],\n      default: null\n    }\n  },\n  data () {\n    return {\n      errors: []\n    }\n  },\n  computed: {\n    syncValue: {\n      get () {\n        return this.value\n      },\n      set (value) {\n        this.$emit('change', value)\n      }\n    }\n  },\n  methods: {\n    onValidatorMounted () {\n      this.$watch(\n        function () {\n          return this.$refs.validator.errors || []\n        },\n        function (errors) {\n          this.errors = errors\n        },\n        { immediate: true, deep: true }\n      )\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.form-input-date-time {\n\n}\n</style>\n"]}]}